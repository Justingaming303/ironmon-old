{
    "name": "ironmon",
    "version": "0.1.0",
    "description": "Ironmon, a discord bot written with TypeScript.",
    "main": "index.ts",
    "author": "Sakari Mursu <sakari@mursu.dev>",
    "scripts": {
        "start": "npm run dev",
        "test": "echo \"Error: no test specified\" && exit 1",
        "dev": "cross-env NODE_ENV=development ts-node-dev --clear --ignore-watch node_modules src/index.ts",
